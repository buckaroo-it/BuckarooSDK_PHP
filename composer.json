{
    "name": "buckaroo/sdk",
    "description": "Buckaroo payment SDK",
    "license": "MIT",
    "version": "1.0.1",
    "type": "library",
    "require": {
        "php": ">=7.4|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-fileinfo": "*",
        "composer/ca-bundle": "^1.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "symfony/var-dumper": "^5.4.9",
        "phpunit/phpunit": "^9.5",
        "vlucas/phpdotenv": "^5.4"
    },
    "suggest": {
        "monolog/monolog": "Allows more advanced logging of the application flow"
    },
    "authors": [{
        "name": "Buckaroo",
        "email": "support@buckaroo.nl",
        "homepage": "https://www.buckaroo.nl",
        "role": "Developer"
    }],
    "autoload": {
        "psr-4": {
            "Buckaroo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BuckarooTests\\": "Tests/"
        }
    }
}
